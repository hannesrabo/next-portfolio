{
	"name": "hrabo-portfolio",
	"scripts": {
		"start": "next",
		"build": "next build ",
		"export": "next export",
		"commit": "npx git-cz"
	},
	"dependencies": {
		"next": "^8.0.1",
		"react": "^16.8.2",
		"react-dom": "^16.8.2",
		"styled-components": "^4.1.3"
	},
	"devDependencies": {
		"cz-conventional-changelog": "^2.1.0",
		"prettier": "^1.16.4",
		"semantic-release": "^15.13.3",
		"semantic-release-gitlab": "^7.1.3"
	},
	"prettier": {
		"semi": false,
		"useTabs": true,
		"singleQuote": true,
		"tabWidth": 4
	},
	"babel": {
		"presets": [
			"next/babel"
		],
		"plugins": [
			[
				"styled-components",
				{
					"ssr": true
				}
			]
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
